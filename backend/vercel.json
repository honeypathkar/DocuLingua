{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "includeFiles": "node_modules/tesseract.js-core/tesseract-core-simd.wasm",
      "maxDuration": 30
    }
  }
}
